package com.callor.hello;

public class HelloTimes {

	public static void main(String[] args) {
		System.out.println("3 * 1 = " + 3 * 1);
		System.out.println("3 * 1 = " + 3 * 2);
		System.out.println("3 * 1 = " + 3 * 3);
		System.out.println("3 * 1 = " + 3 * 4);
		System.out.println("3 * 1 = " + 3 * 5);
		System.out.println("3 * 1 = " + 3 * 6);
		System.out.println("3 * 1 = " + 3 * 7);
		System.out.println("3 * 1 = " + 3 * 8);
		System.out.println("3 * 1 = " + 3 * 9);
		
	}

}
